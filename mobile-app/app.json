{
  "expo": {
    "name": "MindSpace",
    "slug": "mindspace",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mindspace.app",
      "buildNumber": "1.0.0",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mindspace.app",
      "versionCode": 1,
      "permissions": ["INTERNET"]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "name": "MindSpace - Mental Wellness",
      "shortName": "MindSpace",
      "lang": "en-US",
      "startUrl": "/",
      "display": "standalone",
      "backgroundColor": "#ffffff",
      "themeColor": "#0073e6",
      "description": "AI-powered mental wellness platform for youth",
      "dir": "ltr",
      "orientation": "portrait",
      "categories": ["health", "wellness", "lifestyle"],
      "icons": [
        {
          "src": "./assets/pwa/icon-192.png",
          "sizes": "192x192"
        },
        {
          "src": "./assets/pwa/icon-512.png",
          "sizes": "512x512"
        },
        {
          "src": "./assets/pwa/maskable-icon.png",
          "sizes": "512x512",
          "purpose": "maskable"
        }
      ],
      "splash": {
        "image": "./assets/pwa/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      "expo-localization",
      "expo-font"
    ],
    "updates": {
      "fallbackToCacheTimeout": 0,
      "enabled": true
    },
    "extra": {
      "eas": {
        "projectId": "mindspace-app"
      }
    }
  }
}
